<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:id="@+id/linearMain"
    android:orientation="vertical"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_weight="1.5"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_height="match_parent">

    <TextView
        android:id="@+id/result"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_margin="2dp"
        android:gravity="center"
        android:text=""
        android:textSize="14sp"
        android:visibility="gone" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_weight="1"
    android:orientation="horizontal"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/backBtn"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
     android:layout_gravity="center"
        android:background="@drawable/back_arrow" />

    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"

        android:fontFamily="@font/quicksand_bold"
        android:gravity="center"
        android:text="@string/unCollectedCheques_report"
        android:textColor="@color/colorblue1"
        android:textSize="16sp" />
</LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
    <ImageButton
        android:id="@+id/share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
android:layout_gravity="center"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:background="@drawable/share" />

    <ImageButton
        android:id="@+id/excelConvert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_gravity="center"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:layout_toStartOf="@+id/share"
        android:layout_toLeftOf="@+id/share"
        android:background="@drawable/excel" />

    <ImageButton
        android:id="@+id/pdfConvert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_gravity="center"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:layout_toStartOf="@+id/excelConvert"
        android:layout_toLeftOf="@+id/excelConvert"
        android:background="@drawable/pdf" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear1"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/title"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginHorizontal="22dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/from_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:fontFamily="@font/alegreya_sans"
            android:text="@string/customerName"
            android:textColor="#103164"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/cust_select"
            android:layout_width="0dp"
            android:drawableEnd="@drawable/ic_search_blue_24dp"
            android:text="@string/selectcust"
            android:textColor="@color/colorBlack"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_weight="1"
            android:textAlignment="center"
            android:padding="2dp"/>
        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padd_10"
            android:layout_marginLeft="@dimen/padd_10"
            android:layout_weight="0.6"
            android:gravity="center"
            app:boxCornerRadiusBottomEnd="@dimen/padd_10"
            app:boxCornerRadiusBottomStart="@dimen/padd_10"
            app:boxCornerRadiusTopEnd="@dimen/padd_10"
            app:boxCornerRadiusTopStart="@dimen/padd_10"
            app:boxStrokeColor="@color/new_dark_blue"
            app:boxStrokeWidth="1dp"
            android:visibility="gone"
            app:endIconMode="dropdown_menu"
            app:endIconTint="@color/new_dark_blue"
            android:textAlignment="center">

            <AutoCompleteTextView
                android:id="@+id/customers_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"

                android:textAlignment="center"
                android:padding="2dp"/>

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_below="@id/linear1"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginHorizontal="22dp"

        android:gravity="center_vertical"
        android:orientation="horizontal">


        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:fontFamily="@font/alegreya_sans"
            android:text="@string/Paidamount"
            android:textColor="#103164"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/paidAmountText"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="@dimen/padd_10"
            android:layout_marginLeft="@dimen/padd_10"
            android:layout_weight="0.6"
            android:background="@drawable/border_dark_blue"
            android:gravity="center"
            android:padding="8dp"
            android:textColor="@color/gray" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/linear2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginHorizontal="22dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal">


        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.25"
            android:fontFamily="@font/alegreya_sans"
            android:text="@string/Receivedamount"
            android:textColor="#103164"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/recivedAmount_text"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="@dimen/padd_10"
            android:layout_marginLeft="@dimen/padd_10"
            android:layout_weight="0.7"
            android:background="@drawable/border_dark_blue"
            android:gravity="center"
            android:padding="8dp"
            android:textColor="@color/gray" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/preview_button_account"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="@dimen/padd_10"
            android:layout_marginLeft="@dimen/padd_10"
            android:layout_weight="0.3"
            android:background="@drawable/yellow_btn"
            android:fontFamily="@font/alegreya_sans_bold"
            android:text="@string/preview"
            android:textAllCaps="false"
            android:textColor="@color/new_dark_blue"
            android:textSize="16sp" />


    </LinearLayout>
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_weight="1">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_navigation"
        android:layout_below="@id/linear3"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="5dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:background="#76C4FF"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="12dp">


            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/app_bank_name"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/check_number"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/date"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/app_amount"
                android:textColor="@color/white"
                android:textSize="15sp" />

        </LinearLayout>

        <TableLayout
            android:id="@+id/TableCheckData"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="2dp"
            android:background="@color/layer7" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="52dp"

            android:layout_below="@id/TableCheckData"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="12dp">


            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/id"
                android:textColor="#103164"
                android:textSize="15sp" />

            <TextView
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alegreya_sans_bold"
                android:gravity="center"
                android:text="@string/sales_man_name"
                android:textColor="#103164"
                android:textSize="15sp" />

        </LinearLayout>
    </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:id="@+id/tot_lin"
        android:layout_above="@id/bottom_navigation"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="12dp">

        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/alegreya_sans_bold"
            android:gravity="center"
            android:text="@string/total"
            android:textColor="#103164"
            android:textSize="15sp" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/alegreya_sans_bold"
            android:gravity="center"
            android:id="@+id/total"

            android:textColor="#103164"
            android:textSize="15sp" />
    </LinearLayout>

    <include
        android:id="@+id/bottom_navigation"
        layout="@layout/bottom_nav_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp" />


</LinearLayout>

